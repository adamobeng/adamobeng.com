*{margin:0;padding:0}html,body{height:100%}body{background-color:#4d7490;font-family:"adobe-garamond-pro-1", "adobe-garamond-pro-2", serif;*font-size:small;text-align:center;text-color:#f0f0f0}h1,h2,h3,h4,h5,h6{font-size:100%}h1{margin-bottom:1em}p{margin:1em 0}a{color:#0080dd}a:hover{color:black}table{font-size:inherit;font:100%}ul.posts{list-style-type:none;margin-bottom:2em}ul.posts li{line-height:1.75em}ul.posts span{color:#aaa;font-family:Monaco, "Courier New", monospace;font-size:80%}.outer{width:60em;margin:3em auto 2em auto}nav{text-align:right;padding-right:2em;float:left;border-right:1px dashed black;width:4em;margin:3.5em 0 0 0;background-color:#f6f9fa;margin-bottom:2em;font-family:"raleway-1", "raleway-2", sans-serif;border-radius:4px 0 0 4px;box-shadow:-2px 2px 2px 0 #3c596f;-moz-box-shadow:-2px 2px 2px 0 #3c596f;-webkit-box-shadow:-2px 2px 2px 0 #3c596f}nav li{list-style-type:none}nav li a:link{color:#0080dd}nav li a:hover{color:black}nav li a:visited{color:#004577}header{color:black;font-weight:bold;margin:0;border-bottom:1px dashed black;font-size:180%;padding-top:0.5em;padding-bottom:0.5em;font:sans-serif;font-family:"raleway-1", "raleway-2", sans-serif}header h1{margin:0;padding:0}header a{color:#ff7511;text-decoration:none}header a:hover{color:black}header a .extra{text-decoration:none;margin-left:2em}header a .extra:hover{color:black}.site{font-size:110%;text-align:justify;width:40em;margin:3em auto 2em auto;line-height:1.5em;background:#f6f9fa;padding:0 1em 1em 1em;color:#000020;border-radius:10px;-moz-border-radius:10px;-webkit-border-radius:10px;box-shadow:-2px 2px 2px 0 #3c596f;-moz-box-shadow:-2px 2px 2px 0 #3c596f;-webkit-box-shadow:-2px 2px 2px 0 #3c596f}.site .home{height:40em;margin:4em}.site .projects{border:1px dashed #3c596f;width:320px;font-size:90%;float:left;padding:0.5em;border-radius:4px;-moz-border-radius:4px;-webkit-border-radius:4px;height:150px}.site .projects h1{font-family:"raleway-1", "raleway-2", sans-serif}.site .blog{border:1px dashed #3c596f;width:320px;font-size:90%;float:right;padding:0.5em;border-radius:4px;-moz-border-radius:4px;-webkit-border-radius:4px;height:150px}.site .blog h1{font-family:"raleway-1", "raleway-2", sans-serif}.site .meta{color:#aaa}.site footer{font-size:80%;color:#664;border-top:1px dashed black;margin-top:27em;overflow:hidden}.site footer .contact{float:right;margin-right:3em}.site footer .contact a{color:black}.twitter{float:left;padding-top:1em}article pre{border:1px solid #ddd;background-color:#eef;padding:0 .4em}article ul,article ol{margin-left:1.25em}article code{border:1px solid #ddd;background-color:#eef;font-size:95%;padding:0 .2em}article pre code{border:none}article pre.terminal{border:1px solid black;background-color:#333;color:white}article pre.terminal code{background-color:#333}article h1{font-family:"raleway-1", "raleway-2", sans-serif;padding:0 0 0em 0;font-size:90%}article h2{font-family:"raleway-1", "raleway-2", sans-serif;padding:0.5em 0 0 0;font-size:90%}article header{color:#0080dd;font-size:80%;text-align:right;border-bottom:1px dashed #3c596f;font-family:"Monaco", "Courier New", monospace}article header h1{padding:0;margin:0;font-size:140%;text-align:left;color:black}#related{margin-top:2em}#related h2{margin-bottom:1em}.cv{text-align:left;margin:3em auto 2em auto;line-height:1.5em;background:#fefefe;padding:0 1em 1em 1em;width:60em}.cv .cvheader{font-size:20px;color:black;font-variant:small-caps;padding-top:2em}.cv .cvsmall{font-size:90%}.cv ul{list-style-type:none}.cv ul li{padding-left:2em}.cv ul li span{color:#aaa;font-family:Monaco, "Courier New", monospace;font-size:80%;padding-right:2em}.cv h1{font-size:2em;margin-top:1em;padding-top:1em;font-family:"raleway-1", "raleway-2", sans-serif}.cv h3{display:inline}
